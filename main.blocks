ins.i2cWriteBuffer(_I2CAddr, buf7)" line972="    }" line973="" line974="    /**" line975="     * show a number in OLED" line976="     * @param x is X alis, eg: 0" line977="     * @param y is Y alis, eg: 0" line978="     * @param num is the number will be show, eg: 12" line979="     * @param color is number color, eg: 1" line980="     */" line981="    //% blockId=&quot;OLED12864_I2C_NUMBER&quot; block=&quot;显示 数字 在 x %x|y %y|数字 %num&quot;" line982="    //% parts=OLED12864_I2C trackArgs=0" line983="    //% group=&quot;OLED显示屏&quot; subcategory=执行器" line984="    //% weight=80 blockGap=10 color=#0855AA" line985="    export function showNumber(x: number, y: number, num: number, color: number = 1) {" line986="        showString(x, y, num.toString(), color)" line987="    }" line988="" line989="" line990="    export function draw() {" line991="        set_pos()" line992="        pins.i2cWriteBuffer(_I2CAddr, _screen)" line993="    }" line994="" line995="    /**" line996="     * clear screen" line997="     */" line998="    //% blockId=&quot;OLED12864_I2C_CLEAR&quot; block=&quot;清除&quot;" line999="    //% parts=OLED12864_I2C trackArgs=0" line1000="    //% group=&quot;OLED显示屏&quot; subcategory=执行器" line1001="    //% weight=63 blockGap=10 color=#0855AA" line1002="    export function clear() {" line1003="        _screen.fill(0)" line1004="        _screen[0] = 0x40" line1005="        draw()" line1006="    }" line1007="" line1008="    /**" line1009="     * OLED initialize" line1010="     * @param addr is i2c addr, eg: 60" line1011="     */" line1012="    //% blockId=&quot;OLED12864_I2C_init&quot; block=&quot;初始化 OLED 地址为 %addr&quot;" line1013="    //% parts=OLED12864_I2C trackArgs=0" line1014="    //% weight=85 blockGap=10" line1015="    //% group=&quot;OLED显示屏&quot; subcategory=执行器" line1016="    //% blockGap=10  color=#0855AA" line1017="    export function init(addr: number) {" line1018="        _I2CAddr = addr;" line1019="        cmd1(0xAE)       // SSD1306_DISPLAYOFF" line1020