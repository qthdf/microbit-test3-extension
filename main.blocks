);" line458="    }" line459="" line460="    //% blockId=&quot;Readflame&quot; block=&quot;火焰传感器 %flamepin&quot;" line461="    //% subcategory=传感器" line462="    export function Readflame(flamepin: AnalogPin): number {" line463="        let voltage6 = 0;" line464="        let flame = 0;" line465="        voltage6 = pins.map(" line466="            pins.analogReadPin(flamepin)," line467="            0," line468="            1023," line469="            0," line470="            1023" line471="        );" line472="        flame = voltage6;" line473="        return Math.round(flame);" line474="    }" line475="" line476="    //% blockId=&quot;ReadGrayLevel&quot; block=&quot;灰度传感器 %graylevelpin&quot;" line477="    //% subcategory=传感器" line478="    export function ReadGrayLevel(graylevelpin: AnalogPin): number {" line479="        let voltage7 = 0;" line480="        let grayLevel = 0;" line481="        voltage7 = pins.map(" line482="            pins.analogReadPin(graylevelpin)," line483="            0," line484="            1023," line485="            80," line486="            1023" line487="        );" line488="        grayLevel = voltage7;" line489="        return Math.round(grayLevel);" line490="    }" line491="" line492="    //% blockId=&quot;readWaterTemp&quot; block=&quot;防水温度传感器 %waterproofpin&quot;" line493="    //% subcategory=传感器" line494="    export function readWaterTemp(waterproofpin: AnalogPin): number {" line495="        let voltage22 = 0;" line496="        let waterProofTemp = 0;" line497="        voltage22 = pins.analogReadPin(waterproofpin);//%获得原始值" line498="        if (voltage22 &gt; 1001) { waterProofTemp = 100; }" line499="        else if (voltage22 &gt; 1000) { waterProofTemp = 98; }" line500="        else if (voltage22 &gt; 999) { waterProofTemp = 97; }" line501="        else if (voltage22 &gt; 998) { waterProofTemp = 96; }" line502="        else if (voltage22 &gt; 997) { waterProofTemp = 95; }" line503="        else if (voltage22 &gt; 996) { waterProofT