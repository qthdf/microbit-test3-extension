<xml xmlns="https://developers.google.com/blockly/xml"><block type="pxt-on-start" x="10" y="10"><statement name="HANDLER"><block type="typescript_statement" editable="false"><mutation xmlns="http://www.w3.org/1999/xhtml" line0="enum PingUnit {" line1="    //% block=&quot;微秒&quot;" line2="    MicroSeconds," line3="    //% block=&quot;厘米&quot;" line4="    Centimeters," line5="    //% block=&quot;英寸&quot;" line6="    Inches" line7="}" numlines="8"></mutation><next><block type="typescript_statement" editable="false"><mutation xmlns="http://www.w3.org/1999/xhtml" line0="enum PINs {" line1="    P0," line2="    P1," line3="    P2," line4="    P3," line5="    P4," line6="    P5," line7="    P6," line8="    P7," line9="    P8," line10="    P9," line11="    P10," line12="    P11," line13="    P12," line14="    P13," line15="    P14," line16="    P15," line17="    P16," line18="    P19," line19="    P20" line20="}" numlines="21"></mutation><next><block type="typescript_statement" editable="false"><mutation xmlns="http://www.w3.org/1999/xhtml" line0="enum TMP36Type {" line1="    //% block=&quot;(℃)&quot; enumval=0" line2="    TMP36_temperature_C," line3="" line4="    //% block=&quot;(℉)&quot; enumval=1" line5="    TMP36_temperature_F," line6="}" numlines="7"></mutation><next><block type="typescript_statement" editable="false"><mutation xmlns="http://www.w3.org/1999/xhtml" line0="enum RGB {" line1="    //% block=&quot;红&quot;" line2="    RED," line3="    //% block=&quot;绿&quot;" line4="    GREEN," line5="    //% block=&quot;蓝&quot;" line6="    BLUE," line7="    //% block=&quot;全部&quot;" line8="    CLEAR" line9="}" numlines="10"></mutation><next><block type="typescript_statement" editable="false"><mutation xmlns="http://www.w3.org/1999/xhtml" line0="enum NeoPixelColors {" line1="    //% block=红" line2="    Red = 0xFF0000," line3="    //% block=橙" line4="    Orange = 0xFFA500," line5="    //% block=黄" line6="    Yellow = 0xFFFF00," line7="    //% block=绿" line8="    Green = 0x00FF00," line9="    //% block=蓝" line10="    Blue = 0x0000FF," line11="    //% block=靛蓝" line12="    Indigo = 0x4b0082," line13="    //% block=紫罗兰" line14="    Violet = 0x8a2be2," line15="    //% block=紫" line16="    Purple = 0xFF00FF," line17="    //% block=白" line18="    White = 0xFFFFFF," line19="    //% block=黑" line20="    Black = 0x000000" line21="}" numlines="22"></mutation><next><block type="typescript_statement" editable="false"><mutation xmlns="http://www.w3.org/1999/xhtml" line0="enum NeoPixelMode {" line1="    //% block=&quot;RGB (GRB 格式)&quot;" line2="    RGB = 1," line3="    //% block=&quot;RGB+W&quot;" line4="    RGBW = 2," line5="    //% block=&quot;RGB (RGB 格式)&quot;" line6="    RGB_RGB = 3" line7="}" numlines="8"></mutation><next><block type="typescript_statement" editable="false"><mutation xmlns="http://www.w3.org/1999/xhtml" line0="namespace Microbit {" line1="" line2="    export enum DHT11_state {" line3="        //% block=&quot;温度(℃)&quot; enumval=0" line4="        DHT11_temperature_C," line5="" line6="        //% block=&quot;湿度(0~100)&quot; enumval=1" line7="        DHT11_humidity," line8="    }" line9="" line10="    export enum Distance_Unit_List {" line11="        //% block=&quot;厘米&quot; " line12="        Distance_Unit_cm," line13="" line14="        //% block=&quot;英寸&quot;" line15="        Distance_Unit_inch," line16="    }" line17="" line18="" line19="" line20="" line21="" line22="" line23="    //% weight=30 " line24="    //% blockId=&quot;gatorParticle_begin&quot; " line25="    //% block=&quot;心率血氧传感器初始化&quot;" line26="    //% group=&quot;心率血氧传感器&quot; subcategory=传感器" line27="    //% shim=gatorParticle::begin" line28="    export function begin() {" line29="        return" line30="    }" line31="" line32="    /**" line33="    * Reads either the Red or Infrared detection channels.." line34="    */" line35="    //% weight=29 " line36="    //% blockId=&quot;gatorParticle_color&quot; " line37="    //% block=&quot;获取 %LEDToRead 值&quot;" line38="    //% group=&quot;心率血氧传感器&quot; subcategory=传感器" line39="    //% shim=gatorParticle::color" line40="    export function color(type: LEDToRead): number {" line41="        return 0" line42="    }" line43="" line44="    /**" line45="    * Set which LED's we want the sensor to update and read." line46="    */" line47="    //% weight=28" line48="    //% blockId=&quot;gatorParticle_setReadMode&quot;" line49="    //% block=&quot;设置LED的读取模式 %LEDMode&quot;" line50="    //% group=&quot;心率血氧传感器&quot; subcategory=传感器" line51="    //% shim=gatorParticle::setReadMode" line52="    " line53="    export function setReadMode(mode: LEDMode) {" line54="        return" line55="    }" line56="" line57="    /**" line58="    * Set the amplitude of either Red or Infrared LED" line59="    */" line60="    //% weight=27" line61="    //% blockId=&quot;gatorParticle_setAmplitude&quot;" line62="    //% block=&quot;将强度从 %LEDToRead | 设置为 %myBrightness&quot;" line63="    //% group=&quot;心率血氧传感器&quot; subcategory=传感器" line64="    //% shim=gatorParticle::setAmplitude" line65="    " line66="    export function setAmplitude(led: LEDToRead, myBrightness: number) {" line67="        return" line68="    }" line69="" line70="    /**" line71="    * Grab the heartbeat from the sensor in either beats per minute, or an average of the last 4 BPM readings." line72="    */" line73="    //% weight=26" line74="    //% blockId=&quot;gatorParticle_heartbeat&quot;" line75="    //% block=&quot;设置心跳检测方式为%HeartbeatType&quot;" line76="    //% group=&quot;心率血氧传感器&quot; subcategory=传感器" line77="    //% shim=gatorParticle::heartbeat" line78="    export function heartbeat(type: HeartbeatType): number {" line79="        return 0" line80="    }" line81="" line82="" line83="" line84="" line85="" line86="" line87="    //% blockId=&quot;readsoilmoisture&quot; block=&quot;土壤湿度传感器 %soilhumiditypin&quot;" line88="    //% subcategory=传感器" line89="    export function ReadSoilHumidity(soilmoisturepin: AnalogPin): number {" line90="        let voltage = 0;" line91="        let soilmoisture = 0;" line92="        voltage = pins.map(" line93="            pins.analogReadPin(soilmoisturepin)," line94="            0," line95="            1023," line96="            0," line97="            1023" line98="        );" line99="        soilmoisture = voltage;" line100="        return Math.round(soilmoisture);" line101="    }" line102="    //% blockId=&quot;readlightintensity&quot; block=&quot;光敏传感器 %lightintensitypin&quot;" line103="    //% subcategory=传感器" line104="    export function ReadLightIntensity(lightintensitypin: AnalogPin): number {" line105="        let voltage2 = 0;" line106="        let lightintensity = 0;" line107="        voltage2 = pins.map(" line108="            pins.analogReadPin(lightintensitypin)," line109="            0," line110="            1023," line111="            0," line112="            1023" line113="        );" line114="        lightintensity = voltage2;" line115="        return Math.round(1023 - lightintensity);" line116="    }" line117="    /**" line118="    * toggle fans" line119="    */" line120="    //% blockId=fans block=&quot;电机风扇 %pin 切换至 $fanstate || 速度 %speed&quot;" line121="    //% fanstate.shadow=&quot;toggleOnOff&quot;" line122="    //% speed.min=0 speed.max=1023" line123="    //% expandableArgumentMode=&quot;toggle&quot;" line124="    //% subcategory=执行器" line125="    export function motorFan(pin: AnalogPin, fanstate: boolean, speed: number = 1023): void {" line126="        if (fanstate) {" line127="            pins.analogSetPeriod(pin, 1023)" line128="            pins.analogWritePin(pin, Math.map(speed, 0, 1023, 0, 1023))" line129="        }" line130="        else {" line131="            pins.analogWritePin(pin, 0)" line132="            speed = 0" line133="        }" line134="    }" line135="" line136="    /**" line137="    * get Ultrasonic distance" line138="    */" line139="    //% blockId=sonarbit" line140="    //% distance_unit.fieldEditor=&quot;gridpicker&quot;" line141="    //% distance_unit.fieldOptions.columns=2" line142="    //% subcategory=传感器" line143="    //% blockId=sonar_ping block=&quot;超声波传感器 echo %echo trig %trig 单位 %unit&quot;" line144="    export function ping(trig: DigitalPin, echo: DigitalPin, unit: PingUnit, maxCmDistance = 500): number {" line145="        // send pulse" line146="        pins.setPull(trig, PinPullMode.PullNone);" line147="        pins.digitalWritePin(trig, 0);" line148="        control.waitMicros(2);" line149="        pins.digitalWritePin(trig, 1);" line150="        control.waitMicros(10);" line151="        pins.digitalWritePin(trig, 0);" line152="" line153="        // read pulse" line154="        const d = pins.pulseIn(echo, PulseValue.High, maxCmDistance * 58);" line155="" line156="        switch (unit) {" line157="            case PingUnit.Centimeters: return Math.idiv(d, 58);" line158="            case PingUnit.Inches: return Math.idiv(d, 148);" line159="            default: return d;" line160="        }" line161="    }" line162="" line163="    /**" line164="    * toggle led" line165="    */" line166="    //% blockId=LEDR block=&quot;红色 LED %pin 切换到 $ledstate || 亮度 %brightness&quot;" line167="    //% brightness.min=0 brightness.max=1023" line168="    //% ledstate.shadow=&quot;toggleOnOff&quot;" line169="    //% expandableArgumentMode=&quot;toggle&quot;" line170="    //% subcategory=执行器" line171="    export function ledRBrightness(pin: AnalogPin, ledstate: boolean, brightness: number = 1023): void {" line172="        if (ledstate) {" line173="            pins.analogSetPeriod(pin, 1023)" line174="            pins.analogWritePin(pin, Math.map(brightness, 1023, 0, 0, 1023))" line175="        }" line176="        else {" line177="            pins.analogWritePin(pin, 1023)" line178="            brightness = 1023" line179="        }" line180="    }" line181="" line182="    /**" line183="    * toggle led" line184="    */" line185="    //% blockId=LEDG block=&quot;绿色 LED %pin 切换到 $ledstate || 亮度 %brightness&quot;" line186="    //% brightness.min=0 brightness.max=1023" line187="    //% ledstate.shadow=&quot;toggleOnOff&quot;" line188="    //% expandableArgumentMode=&quot;toggle&quot;" line189="    //% subcategory=执行器" line190="    export function ledGBrightness(pin: AnalogPin, ledstate: boolean, brightness: number = 1023): void {" line191="        if (ledstate) {" line192="            pins.analogSetPeriod(pin, 1023)" line193="            pins.analogWritePin(pin, Math.map(brightness, 1023, 0, 0, 1023))" line194="        }" line195="        else {" line196="            pins.analogWritePin(pin, 1023)" line197="            brightness = 1023" line198="        }" line199="    }" line200="" line201="    /**" line202="    * toggle led" line203="    */" line204="    //% blockId=LEDB block=&quot;蓝色 LED %pin 切换到 $ledstate || 亮度 %brightness&quot;" line205="    //% brightness.min=0 brightness.max=1023" line206="    //% ledstate.shadow=&quot;toggleOnOff&quot;" line207="    //% expandableArgumentMode=&quot;toggle&quot;" line208="    //% subcategory=执行器" line209="    export function ledBBrightness(pin: AnalogPin, ledstate: boolean, brightness: number = 1023): void {" line210="        if (ledstate) {" line211="            pins.analogSetPeriod(pin, 1023)" line212="            pins.analogWritePin(pin, Math.map(brightness, 1023, 0, 0, 1023))" line213="        }" line214="        else {" line215="            pins.analogWritePin(pin, 1023)" line216="            brightness = 1023" line217="        }" line218="    }" line219="" line220="    /**" line221="    * toggle led" line222="    */" line223="    //% blockId=LEDY block=&quot;黄色 LED %pin 切换到 $ledstate || 亮度 %brightness&quot;" line224="    //% brightness.min=0 brightness.max=1023" line225="    //% ledstate.shadow=&quot;toggleOnOff&quot;" line226="    //% expandableArgumentMode=&quot;toggle&quot;" line227="    //% subcategory=执行器" line228="    export function ledYBrightness(pin: AnalogPin, ledstate: boolean, brightness: number = 1023): void {" line229="        if (ledstate) {" line230="            pins.analogSetPeriod(pin, 1023)" line231="            pins.analogWritePin(pin, Math.map(brightness, 1023, 0, 0, 1023))" line232="        }" line233="        else {" line234="            pins.analogWritePin(pin, 1023)" line235="            brightness = 1023" line236="        }" line237="    }" line238="" line239="    const PCA9685_ADD = 0x40" line240="    const MODE1 = 0x00" line241="    const LED0_ON_L = 0x06" line242="    const PRESCALE = 0xFE" line243="" line244="    let initialized = false" line245="" line246="    export enum enPos {" line247="        //% blockId=&quot;forward&quot; block=&quot;前进&quot;" line248="        forward = 1," line249="        //% blockId=&quot;stop&quot; block=&quot;后退&quot;" line250="        stop = 2" line251="    }" line252="" line253="    export enum enServo {" line254="        S1 = 0," line255="        S2," line256="        S3," line257="        S4" line258="    }" line259="" line260="    export enum enMotors {" line261="        M1 = 8," line262="        M2 = 10," line263="        M3 = 12," line264="        M4 = 14" line265="    }" line266="" line267="    function i2cwrite(addr: number, reg: number, value: number) {" line268="        let buf = pins.createBuffer(2)" line269="        buf[0] = reg" line270="        buf[1] = value" line271="        pins.i2cWriteBuffer(addr, buf)" line272="    }" line273="" line274="    function i2cread(addr: number, reg: number) {" line275="        pins.i2cWriteNumber(addr, reg, NumberFormat.UInt8BE);" line276="        let val = pins.i2cReadNumber(addr, NumberFormat.UInt8BE);" line277="        return val;" line278="    }" line279="" line280="    function initPCA9685(): void {" line281="        i2cwrite(PCA9685_ADD, MODE1, 0x00)" line282="        setFreq(50);" line283="    }" line284="" line285="    function setFreq(freq: number): void {" line286="        // Constrain the frequency" line287="        let prescaleval = 25000000;" line288="        prescaleval /= 4096;" line289="        prescaleval /= freq;" line290="        prescaleval -= 1;" line291="        let prescale = prescaleval; //Math.Floor(prescaleval + 0.5);" line292="        let oldmode = i2cread(PCA9685_ADD, MODE1);" line293="        let newmode = (oldmode &amp; 0x7F) | 0x10; // sleep" line294="        i2cwrite(PCA9685_ADD, MODE1, newmode); // go to sleep" line295="        i2cwrite(PCA9685_ADD, PRESCALE, prescale); // set the prescaler" line296="        i2cwrite(PCA9685_ADD, MODE1, oldmode);" line297="        control.waitMicros(5000);" line298="        i2cwrite(PCA9685_ADD, MODE1, oldmode | 0xa1);" line299="    }" line300="" line301="    function setPwm(channel: number, on: number, off: number): void {" line302="        if (channel &lt; 0 || channel &gt; 15)" line303="            return;" line304="        if (!initialized) {" line305="            initPCA9685();" line306="        }" line307="        let buf2 = pins.createBuffer(5);" line308="        buf2[0] = LED0_ON_L + 4 * channel;" line309="        buf2[1] = on &amp; 0xff;" line310="        buf2[2] = (on &gt;&gt; 8) &amp; 0xff;" line311="        buf2[3] = off &amp; 0xff;" line312="        buf2[4] = (off &gt;&gt; 8) &amp; 0xff;" line313="        pins.i2cWriteBuffer(PCA9685_ADD, buf2);" line314="    }" line315="" line316="    //% blockId=SuperBit_Servo4 block=&quot;Geek舵机| %num|角度 %value&quot;" line317="    //% num.min=1 num.max=4 value.min=0 value.max=300" line318="    //% name.fieldEditor=&quot;gridpicker&quot; name.fieldOptions.columns=20" line319="    //% subcategory=执行器" line320="    export function Servo4(num: enServo, value: number): void {" line321="" line322="        // 50hz: 20,000 us" line323="        let us = (value * 1800 * 0.6 / 180 + 600); // 0.6 ~ 2.4" line324="        let pwm = us * 4096 / 20000;" line325="        setPwm(num, 0, pwm);" line326="" line327="    }" line328="" line329="    //% blockId=SuperBit_MotorRun block=&quot;电机|%index|速度(-255~255) %speed&quot;" line330="    //% speed.min=-255 speed.max=255" line331="    //% subcategory=执行器" line332="    export function MotorRun(index: enMotors, speed: number): void {" line333="        if (!initialized) {" line334="            initPCA9685()" line335="        }" line336="        speed = speed * 16; // map 255 to 4096" line337="        if (speed &gt;= 4096) {" line338="            speed = 4095" line339="        }" line340="        if (speed &lt;= -4096) {" line341="            speed = -4095" line342="        }" line343="" line344="        let a = index" line345="        let b = index + 1" line346="" line347="        if (a &gt; 10) {" line348="            if (speed &gt;= 0) {" line349="                setPwm(a, 0, speed)" line350="                setPwm(b, 0, 0)" line351="            } else {" line352="                setPwm(a, 0, 0)" line353="                setPwm(b, 0, -speed)" line354="            }" line355="        }" line356="        else {" line357="            if (speed &gt;= 0) {" line358="                setPwm(b, 0, speed)" line359="                setPwm(a, 0, 0)" line360="            } else {" line361="                setPwm(b, 0, 0)" line362="                setPwm(a, 0, -speed)" line363="            }" line364="        }" line365="    }" line366="" line367="    //% blockId=&quot;elecmagnet&quot; block=&quot;电磁铁 %pin 切换至 %magState || 磁力 %force&quot;" line368="    //% magState.shadow=&quot;toggleOnOff&quot;" line369="    //% force.min=0 force.max=1023" line370="    //% expandableArgumentMode=&quot;toggle&quot;" line371="    //% subcategory=执行器" line372="    export function elecmagnet(pin: AnalogPin, magState: boolean, force: number = 1023): void {" line373="" line374="        if (magState) {" line375="            pins.analogSetPeriod(pin, 1023)" line376="            pins.analogWritePin(pin, Math.map(force, 0, 1023, 0, 1023))" line377="        }" line378="        else {" line379="            pins.analogWritePin(pin, 0)" line380="            force = 0" line381="        }" line382="    }" line383="    //% blockId=&quot;pinpullup&quot; block=&quot;巡线传感器 %pin 引脚为上拉&quot;" line384="    //% subcategory=执行器" line385="    export function pinpullup(pin: PINs): void {" line386="        switch (pin) {" line387="            case PINs.P0: pins.setPull(DigitalPin.P0, PinPullMode.PullUp);" line388="            case PINs.P1: pins.setPull(DigitalPin.P1, PinPullMode.PullUp);" line389="            case PINs.P2: pins.setPull(DigitalPin.P2, PinPullMode.PullUp);" line390="            case PINs.P3: pins.setPull(DigitalPin.P3, PinPullMode.PullUp);" line391="            case PINs.P4: pins.setPull(DigitalPin.P4, PinPullMode.PullUp);" line392="            case PINs.P5: pins.setPull(DigitalPin.P5, PinPullMode.PullUp);" line393="            case PINs.P6: pins.setPull(DigitalPin.P6, PinPullMode.PullUp);" line394="            case PINs.P7: pins.setPull(DigitalPin.P7, PinPullMode.PullUp);" line395="            case PINs.P8: pins.setPull(DigitalPin.P8, PinPullMode.PullUp);" line396="            case PINs.P9: pins.setPull(DigitalPin.P9, PinPullMode.PullUp);" line397="            case PINs.P10: pins.setPull(DigitalPin.P10, PinPullMode.PullUp);" line398="            case PINs.P11: pins.setPull(DigitalPin.P11, PinPullMode.PullUp);" line399="            case PINs.P12: pins.setPull(DigitalPin.P12, PinPullMode.PullUp);" line400="            case PINs.P13: pins.setPull(DigitalPin.P13, PinPullMode.PullUp);" line401="            case PINs.P14: pins.setPull(DigitalPin.P14, PinPullMode.PullUp);" line402="            case PINs.P15: pins.setPull(DigitalPin.P15, PinPullMode.PullUp);" line403="            case PINs.P16: pins.setPull(DigitalPin.P16, PinPullMode.PullUp);" line404="            case PINs.P19: pins.setPull(DigitalPin.P19, PinPullMode.PullUp);" line405="            case PINs.P20: pins.setPull(DigitalPin.P20, PinPullMode.PullUp);" line406="        }" line407="" line408="" line409="    }" line410="" line411="    //% blockId=&quot;laser_controller&quot; block=&quot;激光 %